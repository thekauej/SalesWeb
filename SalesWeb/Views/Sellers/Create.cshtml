@model SalesWeb.Models.Seller
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Create";
}

<h2>@ViewData["Title"]</h2>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <strong>
                    <label asp-for="Name" class="col-form-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <label asp-for="Email" class="col-form-label"></label>
                    <input asp-for="Email" class="form-control" />
                    <label asp-for="BirthDate" class="col-form-label"></label>
                    <input asp-for="BirthDate" class="form-control" />
                    <label asp-for="BaseSalary" class="col-form-label"></label>
                    <input asp-for="BaseSalary" class="form-control" />
                </strong>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-secondary" />
            </div>
        </form>
    </div>
</div>